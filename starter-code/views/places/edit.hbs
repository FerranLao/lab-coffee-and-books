{{#if error}}
<h2>{{error}}</h2>
{{/if}}
<form id="form" action="/places/{{place._id}}/edit" method="POST">
    <label for="name">Place name</label><br>
    <input type="text" name="name" value="{{place.name}}">
    <br><br>
    <select name="type">
        <option selected value="coffe shop"> Coffe Shop </option>
        <option value="bookstore">Bookstore</option>
    </select>
    <br><br>
    <input type="text" name="latitude" value={{latitude}}>
    <input type="text" name="longitude" value={{longitude}}>
    <button>Edit this place</button>
</form>
<div id="map" class="map"></div>
